{
    "name": "starter-mastergo-vue",
    "version": "1.0.0",
    "author": "river <laolei@forkway.cn>",
    "license": "MIT",
    "description": "A Starter Template For masterGo Plugin Power By Vue",
    "repository": "https://github.com/leizhenpeng/starter-mastergo-vue",
    "type": "module",
    "engines": {
        "node": "^14.18.0 || >=16.0.0"
    },
    "packageManager": "pnpm@7.9.5",
    "keywords": [
        "masterGo",
        "masterGo-plugin",
        "starter",
        "vue",
        "vite"
    ],
    "scripts": {
        "dev:ui": "LIB_NAME=ui vite",
        "dev:hook": "vue-tsc --noEmit && LIB_NAME=hook vite build --mode production --watch",
        "build:hook": "vue-tsc --noEmit && LIB_NAME=hook vite build --mode production",
        "build:ui": "vue-tsc --noEmit && LIB_NAME=ui vite build --mode production",
        "build": "npm run build:hook && npm run build:ui && node utils/postProcessHtml.js",
        "preview": "vite preview",
        "test": "vitest"
    },
    "dependencies": {
        "@mastergo/plugin-utils": "^0.1.0",
        "@vueuse/core": "^9.2.0",
        "eventemitter3": "^4.0.7",
        "figx": "^0.1.0",
        "lodash": "^4.17.21",
        "naive-ui": "^2.33.3",
        "vue": "^3.2.39"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.25.2",
        "@iconify-json/ph": "^1.1.0",
        "@mastergo/plugin-typings": "^1.0.0",
        "@unocss/preset-mini": "^0.45.21",
        "@unocss/reset": "^0.45.21",
        "@vitejs/plugin-vue": "^2.3.4",
        "eslint": "^8.19.0",
        "less": "^4.1.2",
        "rollup-obfuscator": "^3.0.1",
        "terser": "^5.15.0",
        "typescript": "^4.8.3",
        "unocss": "^0.45.21",
        "unplugin-auto-import": "^0.9.2",
        "unplugin-vue-components": "^0.21.1",
        "vite": "^2.9.15",
        "vite-node": "^0.17.1",
        "vitest": "^0.18.1",
        "vue-tsc": "^1.0.1"
    }
}
