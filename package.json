{
    "name": "starter-mastergo-vue",
    "version": "1.0.0",
    "author": "river <laolei@forkway.cn>",
    "license": "MIT",
    "description": "A Starter Template For masterGo Plugin Power By Vue",
    "repository": "https://github.com/leizhenpeng/starter-mastergo-vue",
    "type": "module",
    "engines": {
        "node": "^14.18.0 || >=16.0.0"
    },
    "packageManager": "pnpm@7.9.5",
    "keywords": [
        "masterGo",
        "masterGo-plugin",
        "starter",
        "vue",
        "vite"
    ],
    "watch": {
        "process": "dist/*/*.js"
    },
    "scripts": {
        "dev:ui": "LIB_NAME=ui vite",
        "dev:hook": "vue-tsc --noEmit && LIB_NAME=hook vite build --mode production --watch",
        "dev:deep:ui": "vue-tsc --noEmit && LIB_NAME=ui vite build --mode production --watch",
        "process": "node utils/postProcessHtml.js",
        "watch:process": "npm-watch process",
        "start:dev": "npm-run-all --parallel dev:** watch:process --continue-on-error",
        "build:hook": "vue-tsc --noEmit && LIB_NAME=hook vite build --mode production",
        "build:ui": "vue-tsc --noEmit && LIB_NAME=ui vite build --mode production&& node utils/postProcessHtml.js ",
        "build": "npm run build:hook && npm run build:ui",
        "preview": "vite preview",
        "test": "vitest"
    },
    "dependencies": {
        "@mastergo/plugin-utils": "^0.1.11",
        "@vueuse/core": "^9.3.1",
        "eventemitter3": "^4.0.7",
        "figx": "^0.1.0",
        "lodash": "^4.17.21",
        "naive-ui": "^2.33.5",
        "vue": "^3.2.41",
        "vue-router": "^4.1.5"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.25.2",
        "@iconify-json/ph": "^1.1.2",
        "@mastergo/plugin-typings": "^1.3.1",
        "@unocss/preset-mini": "^0.45.29",
        "@unocss/reset": "^0.45.29",
        "@vitejs/plugin-vue": "^2.3.4",
        "eslint": "^8.25.0",
        "less": "^4.1.3",
        "rollup-obfuscator": "^3.0.1",
        "terser": "^5.15.1",
        "typescript": "^4.8.4",
        "unocss": "^0.45.29",
        "unplugin-auto-import": "^0.9.5",
        "unplugin-vue-components": "^0.21.2",
        "vite": "^2.9.15",
        "vite-node": "^0.17.1",
        "vitest": "^0.18.1",
        "vue-tsc": "^1.0.8",
        "vite-svg-loader": "^3.6.0",
        "npm-run-all": "^4.1.5",
        "npm-watch": "^0.11.0"
    }
}
